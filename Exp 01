#include <stdio.h>

int main() {
    int A[2][2] = { {1, 2}, {4, 5} };
    int B[2][2] = { {9, 10}, {11, 12} };
    int C[2][2];
    int i, j, k;

    for (i = 0; i < 2; i++)
        for (j = 0; j < 2; j++) {
            C[i][j] = 0;
            for (k = 0; k < 2; k++)
                C[i][j] += A[i][k] * B[k][j];
        }


    for (i = 0; i < 2; i++) {
        for (j = 0; j < 2; j++)
            printf("%d\t", C[i][j]);
        printf("\n");
    }

    return 0;
}
