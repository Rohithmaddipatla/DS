#include <stdio.h>
#define SIZE 5

int q[SIZE], front = -1, rear = -1;

void enqueue(int val) {
    if (rear == SIZE - 1)
        printf("Queue Full\n");
    else {
        if (front == -1) front = 0;
        rear++;
        q[rear] = val;
    }
}

void dequeue() {
    if (front == -1)
        printf("Queue Empty\n");
    else {
        printf("Deleted: %d\n", q[front]);
        if (front == rear)
            front = rear = -1;
        else
            front++;
    }
}

void display() {
    int i;
    if (front == -1)
        printf("Queue Empty\n");
    else {
        for (i = front; i <= rear; i++)
            printf("%d ", q[i]);
        printf("\n");
    }
}

int main() {
    int ch, val;
    do {
        printf("1.Enqueue 2.Dequeue 3.Display 0.Exit: ");
        scanf("%d", &ch);
        if (ch == 1) {
            printf("Enter value: ");
            scanf("%d", &val);
            enqueue(val);
        } else if (ch == 2) {
            dequeue();
        } else if (ch == 3) {
            display();
        }
    } while (ch != 0);
    return 0;
}

